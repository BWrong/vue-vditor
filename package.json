{
  "name": "vue-vditor",
  "version": "0.0.4",
  "author": "bwrong<ibwrong@foxmail.com>",
  "license": "MIT",
  "keywords": [
    "markdown",
    "vue",
    "vditor",
    "editor"
  ],
  "description": "基于vditor实现的vue版本的markdown编辑器",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "release": "npm run build && standard-version --infile CHANGELOG.md "
  },
  "main": "./lib/vue-vditor.umd.js",
  "module": "./lib/vue-vditor.es.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/vue-vditor.es.js",
      "require": "./lib/vue-vditor.umd.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/BWrong/vue-vditor.git",
  "bugs": {
    "url": "https://github.com/BWrong/vue-vditor.git/issues",
    "email": "ibwrong@foxmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BWrong/vue-vditor.git"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.10",
    "vditor": "~3.8.5",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^15.12.3",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.2",
    "vditor": "^3.8.5",
    "vite": "^2.3.7",
    "vite-plugin-vue2": "^1.6.2",
    "vue": "^3.1.1",
    "vue-tsc": "^0.0.24"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": false
    }
  }
}
